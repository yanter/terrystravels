{
	"name": "dawnlight",
	"type": "module",
	"version": "4.2.0",
	"scripts": {
		"dev": "astro dev",
		"winsearch": "robocopy dist/pagefind public/pagefind /E",
		"osxsearch": "cp -r dist/pagefind public/",
		"start": "astro dev",
		"build": "astro build && pagefind --site dist",
		"preview": "astro preview",
		"astro": "astro",
		"config-i18n": "node ./scripts/config-i18n.js",
		"remove-keystatic": "node ./scripts/remove-keystatic.js",
		"format": "eslint . --fix && prettier -w \"**/*\" --ignore-unknown --cache",
		"lint": "eslint ."
	},
	"dependencies": {
		"@astrojs/mdx": "4.2.6",
		"@astrojs/netlify": "6.3.2",
		"@astrojs/react": "4.2.7",
		"@astrojs/rss": "4.0.11",
		"@astrojs/sitemap": "3.3.1",
		"@keystatic/astro": "5.0.6",
		"@keystatic/core": "0.5.47",
		"@pagefind/default-ui": "1.3.0",
		"@tabler/icons": "3.31.0",
		"@tailwindcss/forms": "0.5.10",
		"@tailwindcss/vite": "4.1.6",
		"@types/react": "18.3.12",
		"@types/react-dom": "18.3.1",
		"animejs": "3.2.2",
		"astro": "5.7.12",
		"astro-auto-import": "0.4.4",
		"astro-icon": "1.1.5",
		"astro-seo": "0.8.4",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"sharp": "^0.34.5",
		"tailwindcss": "4.1.6",
		"tw-animate-css": "1.2.9"
	},
	"devDependencies": {
		"@eslint/js": "9.26.0",
		"@playform/compress": "0.1.9",
		"eslint": "9.26.0",
		"eslint-plugin-astro": "1.3.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-prettier": "5.4.0",
		"eslint-plugin-simple-import-sort": "12.1.1",
		"globals": "16.1.0",
		"lodash.debounce": "4.0.8",
		"lodash.throttle": "4.1.1",
		"pagefind": "1.3.0",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-tailwindcss": "0.6.11",
		"typescript-eslint": "8.32.0"
	},
	"overrides": {
		"sharp": "$sharp"
	}
}
